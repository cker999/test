CREATE OR REPLACE PROCEDURE JONAVA.TEST_PROCEDURE ( P_SUBST_ID IN NUMBER,
   P_CONTR_ID IN NUMBER,
   P_RECORD IN VARCHAR2)
   IS
BEGIN
    INSERT INTO CONTROLLER_LOG
    (SUBST_ID, CONTR_ID, DATETIME, RECORD, TYPE_, RECNO, COMMNO, RECORD_ID)
    VALUES(P_SUBST_ID, P_CONTR_ID, SYSDATE, P_RECORD, 1, CONTRLOG_RECNO_SEQ.NEXTVAL,
    CONTRLOG_COMMNO_SEQ.NEXTVAL, 5);

    COMMIT;
EXCEPTION
    WHEN others THEN
        null;
END;

/
