CREATE OR REPLACE PROCEDURE JONAVA.ADD_ERROR
  ( Err_Msg_In IN VARCHAR2,
    Err_Level_In IN NUMBER:= 1,
	User_In IN VARCHAR2:= USER,
	Module_In IN VARCHAR2:= 'Unknown')
  IS
  PRAGMA AUTONOMOUS_TRANSACTION;
BEGIN
--test3
  INSERT INTO ERROR_LOG (DATETIME, MSG, MSG_LEVEL, SENDER, MODULE)
    VALUES (SYSDATE, Err_Msg_In, Err_Level_In, User_In, Module_In);
  COMMIT;
END;
/
